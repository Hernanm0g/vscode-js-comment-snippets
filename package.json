{
    "name": "vscode-js-comment-snippets",
    "displayName": "VSCode JS Comment Snippets",
    "description": "A port of the famous Sublime Snippets package for Javascript, React, vue and Typescript",
    "version": "1.1.0",
    "publisher":"zer0th",
    "icon":"icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Hernanm0g/vscode-js-comment-snippets.git"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/js-snippets.json"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/js-snippets.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/js-snippets.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/js-snippets.json"
            },
            {
                "language": "vue",
                "path": "./snippets/js-snippets.json"
            },
            {
                "language": "python",
                "path":"./snippets/py-snippets.json"

            }
        ]
    }
}
