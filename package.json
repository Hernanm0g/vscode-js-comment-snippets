{
    "name": "vscode-js-comment-snippets",
    "displayName": "VSCode JS Comment Snippets",
    "description": "A JS port of the famous Sublime Snippets package for VS",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/js-snippets.code-snippets"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/js-snippets.code-snippets"
            },
            {
                "language": "typescript",
                "path": "./snippets/js-snippets.code-snippets"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/js-snippets.code-snippets"
            },
            {
                "language": "vue",
                "path": "./snippets/js-snippets.code-snippets"
            },
        ]
    }
}
